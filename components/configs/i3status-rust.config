[[block]]
block = "battery"
format = " $percentage $time"
interval = 60
missing_format = ""
model = "Framewo"

[[block]]
block = "cpu"
interval = 1
format = "$utilization"

[[block]]
block = "disk_space"
path = "/"
format = "DISK $used/$total"

[[block]]
block = "external_ip"
format = "IP $ip"

[[block]]
block = "memory"
format = "MEM $mem_used/$mem_total"
format_alt = "SWAP $swap_used/$swap_total"
interval = 5

[[block]]
block = "net"
format = "AP $ssid $strength"
format_alt = "NET $name $ap $ips"
[[block.click]]
button = "left"
cmd = "alacritty -e nmtui"

[[block]]
block = "sound"

[[block]]
block = "backlight"
cycle = []
device = "intel_backlight"

[[block]]
block = "time"
format = " $timestamp.datetime(f:'%Y/%m/%d %H:%M:%S') "
timezone = "America/New_York"
interval = 1
